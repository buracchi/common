@PACKAGE_INIT@

set(_supported_components concurrency containers iterators networking statemachine utilities)

foreach(_comp ${${PROJECT_NAME}_FIND_COMPONENTS})
  if (NOT _comp IN_LIST _supported_components)
    set(${PROJECT_NAME}_FOUND False)
    set(${PROJECT_NAME}_NOT_FOUND_MESSAGE "Unsupported component: ${_comp}")
  endif()
  include("${CMAKE_CURRENT_LIST_DIR}/${PROJECT_NAME}_${_comp}Targets.cmake")
endforeach()
